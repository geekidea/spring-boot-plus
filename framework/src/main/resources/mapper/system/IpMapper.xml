<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="io.geekidea.springbootplus.framework.system.mapper.IpMapper">

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        ip_start, ip_end, area, operator, id, ip_start_num, ip_end_num
    </sql>

    <select id="getIpById" resultType="io.geekidea.springbootplus.framework.system.vo.IpQueryVo">
        select
        <include refid="Base_Column_List"/>
        from ip where id = #{id}
    </select>

    <select id="getIpPageList" resultType="io.geekidea.springbootplus.framework.system.vo.IpQueryVo">
        select
        <include refid="Base_Column_List"/>
        from ip
    </select>

</mapper>
